  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <HBox prefHeight="31.0" prefWidth="1320.0">
      <children>
         <Label fx:id="labelListaProcesos" prefHeight="25.0" prefWidth="1198.0" text="Lista de procesos" />
         <MFXButton fx:id="btnCargarProcesos" onAction="#onBtnCargarProcesosClick" prefHeight="26.0" prefWidth="140.0" styleClass="primary-button" text="Cargar procesos" />
      </children>
   </HBox>

   <HBox prefHeight="748.0" prefWidth="1320.0">
      <children>
         <MFXListView fx:id="listProcesos" prefHeight="304.0" prefWidth="1320.0" />
      </children></HBox>
</VBox>
